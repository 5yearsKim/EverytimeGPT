{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "indent": ["warn", 2],
      "no-unused-vars": "warn",
      "semi": "warn",
      "quotes": ["warn", "single"],
      "no-trailing-spaces": "warn",
      "react/prop-types": "off"
    }
}
